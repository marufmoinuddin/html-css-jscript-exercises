height: 200px;
width: 100%;
object-fit: cover;
object-position: 50% 30%;
}

#container{
margin: auto;
width: 1200px;
padding: 10px;
background-color: #D6E6F2;
min-height: 94vh;
}

#genderm, #genderf{
width: 50px;
padding-top: 10px;
}