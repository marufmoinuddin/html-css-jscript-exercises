body{
	color: #4C4C4C;
}

input{
width: 350px;
background-color: #F7FBFC;
border-color: white;
border-style:solid;
font-size: 14px;
padding: 3px;
}

input:focus{
	border-color: #769FCD;
	outline: none!important;
}

.center{
margin: auto;
width: 50%;
padding: 10px;
}

