body{
	color: #4C4C4C;
}

input{
	width: 350px;
	background-color: #F7FBFC;
	border-color: white;
	border-style:solid;
	font-size: 14px;
	padding: 3px;
}

input:focus{
	border-color: #769FCD;
	outline: none!important;
}

button{
	background-color: #769FCD;
	border: none;
	color: white;
	padding: 5px 10px;
	text-align: center;
	text-decoration: none;
	display: inline-block;
	margin: 4px 2px;
	cursor: pointer;
}

button:hover{
	background-color: #8cb6e6;
}
button:active {
	transform: scale(0.95);
}

a{
	text-decoration: None;
}

.center{
	margin: auto;
	width: 50%;
	padding: 10px;
}

.menu td{
	width: 250px;
	text-align: right;
	font-size: 18px;
}

.center-all {
	position: fixed;
	top: 50%;
	left: 50%;
	/* bring your own prefixes */
	transform: translate(-50%, -50%);
}

#bannerpic{
	height: 200px;
	width: 100%;
	object-fit: cover;
	object-position: 50% 30%;
	padding-bottom: 5px;
}

#container{
	margin: auto;
	width: 1200px;
	padding: 10px;
	background-color: #D6E6F2;
	min-height: 94vh;
	position: relative;
}

#nav{
	padding-left: 10px;
	padding-right: 10px;
	padding-top: 5px;
	padding-bottom: 5px;
	background-color: pink;
}

#footer{
	position:absolute;
	bottom:10;
	left:auto;
	right: auto;
	width: 96.5%;
	--margin-top: 104px;
	background-color: #F7FBFC;
	padding: 10px;
	text-align: center;
}